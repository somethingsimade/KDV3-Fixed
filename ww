local DetectedMode
local HatModes = {
	"Just Grass Only",
	"Kass's Cheap",
	"Lay's Chips",
	"Tap's Idea",
	"Not Last Sword",
	"No Hats",
	"Expensive Ass Mode",
	"Sized Up Wing Scale",
    "Sized Up Wing Scale Again",
    "Cool Swords Scaled Up"
}
if HatMode ~= 0 then
	DetectedMode = HatModes[HatMode]
else
	DetectedMode = "None."
	getfenv().Wings=true
end

function Library.Notification(title, text, icon, duration)
	local titleyes = title or "Notification"
	local textyes = text or "library made by typicalusername"
	local iconyes = icon or nil
	local durationyes = duration or 5
	--game.Players:GetUserThumbnailAsync(game.Players.LocalPlayer.UserId, "HeadShot", Enum.ThumbnailSize.Size100x100);
	game.StarterGui:SetCore("SendNotification",  {
		Title = titleyes;
		Text = textyes;
		Icon = iconyes;
		Duration = durationyes;
	})
end


Library.Notification("LAST STAR", "Converted By Anggun, Edited by Lay. \nThe Current Hat Mode Is ".. DetectedMode)

if HatMode == 1 then
justgrassonly()
elseif HatMode == 2 then
cheapomode()
elseif HatMode == 3 then
layschips()
elseif HatMode == 4 then
tapsidea()
elseif HatMode==5 then
notlastsword()
elseif HatMode==6 then
expensiveass()
elseif HatMode==7 then
killyourselfnow()
elseif HatMode==8 then
whac()
elseif HatMode==9 then
whacagain()
elseif HatMode==10 then
colorful()
else
print("NoHats (ur broke)")
end

end

