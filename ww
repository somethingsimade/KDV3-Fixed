whac=function()
local HATDUPES = Library.MakeTableOfHats("Mesh",{Mesh_Id=11279417526})
local HATDUPESAGAIN = Library.MakeTableOfHats("Mesh",{Mesh_Id=7442755030})
local HATDUPESAGAINBRUH = Library.MakeTableOfHats("Mesh",{Mesh_Id=4307568890})

	if HATDUPES[1] then
		local hat = HATDUPES[1]
		hat.Handle.AccessoryWeld:Remove()
		alignn(hat.Handle, wing1.Primary, Vector3.new(6.75,0.1,0), Vector3.new(0,0,30.5))
		table.remove(HATDUPES, 1)
		print('aligned')
	end
	if HATDUPES[1] then
		local hat = HATDUPES[1]
		hat.Handle.AccessoryWeld:Remove()
		alignn(hat.Handle, wing1.Primary, Vector3.new(-3,-0.075,0), Vector3.new(0,0,-150))
		table.remove(HATDUPES, 1)
		print('aligned')
	end
	if HATDUPESAGAIN[1] then
		local hat = HATDUPESAGAIN[1]
		hat.Handle.AccessoryWeld:Remove()
		alignn(hat.Handle, wing1.Primary, Vector3.new(1.9,0.12,0), Vector3.new(0, 0, 0))
		table.remove(HATDUPESAGAIN, 1)
		print('aligned')
	end
