			if workspace:FindFirstChild("Chaos Spire") then
				local mag =( workspace["Chaos Spire"].Speen.Crystal.Position-workspace.CurrentCamera.CFrame.Position).Magnitude
				local y = math.clamp(mag,0,3000)
