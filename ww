		end
			if Torso:FindFirstChild("Neck") then
				if anim.Value == "STARTUP" then
					g = .05
					RootJoint.C0 = RootJoint.C0:Lerp(RootCF*cf(0,0.3,0)*angles(math.rad(-5.9),math.rad(0),math.rad(-30.7)),g)
