			gui.Music.CurrentTime.TextStrokeColor3 = ts3
			gui.Music.Length.TextColor3 = t3
			gui.Music.Length.TextStrokeColor3 = ts3
			gui.Music.SongName.TextColor3 = t3
			gui.Music.SongName.TextStrokeColor3 = ts3
			
