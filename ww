cheapomode = function()
	_G.HideWings = true
	local WingHats2 = {}
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Accessory") and v.Name:find("InternationalFedora") then
			table.insert(WingHats2,v)
			--v.Handle.AccessoryWeld:Remove()
		end
	end
	if WingHats2[1] then
		local hat = WingHats2[1]
		hat.Handle.AccessoryWeld:Remove()
		Library.RemoveMesh(hat.Handle)
		alignn(hat.Handle, wing1.Spin, Vector3.new(0,0,0), Vector3.new(-0, 0, 0))
		table.remove(WingHats2, 1)
		--wait()
		print('aligned')
	end
	if WingHats2[1] then
		local hat = WingHats2[1]
		hat.Handle.AccessoryWeld:Remove()
		Library.RemoveMesh(hat.Handle)
		alignn(hat.Handle, wing3.Spin, Vector3.new(0,0,0), Vector3.new(-0, 0, 0))
		table.remove(WingHats2, 1)
		--wait()
		print('aligned')
	end
	if WingHats2[1] then
		local hat = WingHats2[1]
		hat.Handle.AccessoryWeld:Remove()
		Library.RemoveMesh(hat.Handle)
		alignn(hat.Handle, wing4.Spin, Vector3.new(0,0,0), Vector3.new(-0, 0, 0))
