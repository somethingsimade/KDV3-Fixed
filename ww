local baseformtable = {
	["APEX"]=function() if altwheel == false then formswitch(C3R(0,0,0),C3R(255,0,0),6454152476,C3R(255,0,0),C3R(0,0,0),16,50,1,"APEX","Antique","APEX","APEX",.4,true,0,true,"LASTâ˜…STAR",false,"Srav3R & Numb'n'dub // Bomb The Rave",false,"rbxassetid://6593238379") chatmsg(apexlines[rand(1,3)]) end end;
	["REPRESSION"]=function() if altwheel == false then formswitch(C3R(0,0,0),C3R(255,130,0),4777200099,C3R(255,100,0),C3R(0,0,0),20,40,1,"REPRESSION","Oswald","REPRESSION","REPRESSION",.4,true,0,true,"LASTâ˜…STAR",false,"Zomboy // Battlefields",false,"rbxassetid://6593357327") chatmsg(repressionlines[rand(1,3)]) end end;
