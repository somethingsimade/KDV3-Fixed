game:GetService("RunService").RenderStepped:Connect(function()
--   Test= 0 + (-Theme.PlaybackLoudness/75);
	function rotateRing1()
		if SwordAuraAlign[1].Orientation ~= Vector3.new(0, 0, -90) then
			SwordAuraAlign[1].Orientation = SwordAuraAlign[1].Orientation + Vector3.new(0, -0, -5)
		else
			SwordAuraAlign[1].Orientation = Vector3.new(0, 0, 0)
		end
	end

	function rotateRing2()
		if ScytheAlign[1].Orientation ~= Vector3.new(0, 0, 90) then
			ScytheAlign[1].Orientation = ScytheAlign[1].Orientation + Vector3.new(0, -0, 50)
		else
			ScytheAlign[1].Orientation = Vector3.new(0, 0, 0)
		end
	end

	pcall(rotateRing1)
	pcall(rotateRing2)
end)-- InternationalFedora
justgrassonly = function()
	_G.HideWings = true
	local WingHats = Library.MakeTableOfHats("NameOnly",{Name="DemonGodSword"})
	--[[for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Accessory") and v.Name:find("DemonGodSword") then
			table.insert(WingHats,v)
			v.Handle.AccessoryWeld:Remove()
		end
	end]]
	for i,v in pairs(WingHats) do v.Handle.AccessoryWeld:Remove() end
	if WingHats[1] then
		local hat = WingHats[1]
		--hat.AccessoryWeld:Remove()
		alignn(hat.Handle, wing1.Primary, Vector3.new(-3,0,0), Vector3.new(-40, 90, 0))
		table.remove(WingHats, 1)
		--wait()
		print('aligned')
	end
	if WingHats[1] then
		local hat = WingHats[1]
		--hat.AccessoryWeld:Remove()
		alignn(hat.Handle, wing1.Primary, Vector3.new(4,0,0), Vector3.new(-40, 90, 0))
		table.remove(WingHats, 1)
		--wait()
		print('aligned')
	end

	if WingHats[1] then
		local hat = WingHats[1]
		--hat.AccessoryWeld:Remove()
