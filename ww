getfenv().Wings=false
local Humanoids = {}
local CanChat = false
workspace.FallenPartsDestroyHeight = 0/0

for i,v in pairs(workspace:GetDescendants()) do
	if v:IsA("Humanoid") then
		table.insert(Humanoids, v)
	end
end

workspace.DescendantAdded:Connect(function(hum)
	if hum:IsA("Humanoid") then
		table.insert(Humanoids, hum)
	end
end)

workspace.DescendantRemoving:Connect(function(hum)
	if hum:IsA("Humanoid") then
		table.remove(Humanoids, table.find(Humanoids, hum))
	end
end)

function LoadLocalScript(script) -- for loading localscripts
	local FakeEnv = {}
	local ScriptEnv = {script = script}
	local MtEnv = {}
	MtEnv.__index = function(self, key)
		if ScriptEnv[key] then
			return ScriptEnv[key]
		else
			return getfenv()[key]
		end
	end
	MtEnv.__newindex = function(self, key, value)
		if ScriptEnv[key] then
			ScriptEnv[key] = value
		else
			getfenv()[key] = value
		end
	end
	local LoadedEnv = setmetatable(FakeEnv, MtEnv)
	local func = loadstring(script.Source)
	setfenv(func, LoadedEnv)
	return func()
end

function CreateRemote(parent) -- for remotes
    local Main = newproxy(true)
    local MainMt = getmetatable(Main)
    local ServerEvent = Instance.new("BindableEvent")
    local ClientEvent = Instance.new("BindableEvent")
    local FakeRemote = Instance.new("RemoteEvent", parent)

    MainMt.__namecall = function(self, ...)
        local method = getnamecallmethod()
        if method == "FireServer" then
            ServerEvent:Fire(game.Players.LocalPlayer, ...)
            return
        end
        if method == "FireClient" then
            local function seperate(plr, ...)
                return (...)
            end
            local yes = seperate(...)
            ClientEvent:Fire(yes)
            return
        end
        if method == "FireAllClients" then
            ClientEvent:Fire(...)
			return
        end
        return getrawmetatable(game).__namecall(FakeRemote, ...)
    end
        
    MainMt.__index = function(self, key)
        if key == "OnServerEvent" then   
            return ServerEvent.Event
        end
        if key == "OnClientEvent" then   
            return ClientEvent.Event
        end
        return FakeRemote[key]
    end

    MainMt.__newindex = function(self, key, val)
        FakeRemote[key] = val
    end
    return Main
end

